{
  "name": "@sync/ai",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'No linting configured for AI package'",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:evaluation": "jest tests/evaluation.test.ts --verbose",
    "test:schema-compliance": "jest tests/therapist-prompt.test.ts --testNamePattern='schema|validation' --verbose",
    "test:prompt-variants": "jest tests/therapist-prompt.test.ts --testNamePattern='25 Canned Scenarios' --verbose",
    "generate-eval-report": "tsx src/scripts/generate-eval-report.ts",
    "analyze-survey-correlation": "tsx src/scripts/analyze-survey-correlation.ts",
    "check-compliance-threshold": "tsx src/scripts/check-compliance-threshold.ts",
    "run-ab-test": "tsx src/scripts/run-ab-test.ts",
    "prompt-version-eval": "tsx src/scripts/prompt-version-eval.ts"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "openai": "^4.20.1",
    "zod": "^3.22.4",
    "@sync/types": "file:../../packages/types"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2",
    "tsx": "^4.1.4",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  }
}
